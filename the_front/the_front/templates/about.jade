extends layout

block content
  .row
    .col-sm-6.col-sm-offset-3
      h1.text-center about
      p=info.about
  .row
    br
    br
  .row
    .col-sm-4.col-sm-offset-2
      h3 front members
      {% for artist in artists %}
      .col-sm-12
        a(href='#{artist.get_absolute_url()}') #{artist.name}
      {% endfor %}
    .col-sm-5
      img.img-thumbnail(src="#{STATIC_URL}img/TheFront.jpg")
  .row
    br
    br
  .row
    .col-sm-8.col-sm-offset-2
      h3.text-center FAQ About How The Front Works
      {% for faq in faqs %}
      .row
        .col-xs-12
          h5 #{faq.topic}:
          p=faq.answer
      {% if not forloop.last %}
      .row
        .col-xs-4.col-xs-offset-4
          hr.dashed
      {% endif %}
      {% endfor %}
